generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model CommandLog {
  id         String   @id @default(cuid())
  username   String?
  owner      String?
  repo       String?
  command    String
  status     String
  output     String?
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
